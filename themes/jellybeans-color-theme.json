{
  "name": "jellybeans",
  "type": "dark",
  "colors": {
    "editor.background": "#151515",
    "editor.foreground": "#e8e8d3",
    "sideBar.background": "#151515",
    "sideBarTitle.foreground": "#e8e8d3",
    "sideBar.border": "#3b3b3b",
    "statusBar.background": "#3b3b3b",
    "activityBar.background": "#151515",
    "activityBar.border": "#3b3b3b"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#888888",
        "fontStyle": "italic"
      }
    },
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "#99ad6a"
      }
    },
    {
      "name": "Parameters / Arguments begin and end",
      "scope": [
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end"
      ],
      "settings": {
        "foreground": "#e8e8d3"
      }
    },
    {
      "name": "Parameters / Arguments",
      "scope": [
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#e8e8d3"
      }
    },
    {
      "name": "Pseudo Method",
      "scope": [
        "keyword.control.pseudo-method",
        "keyword.control.flow",
        "keyword.control.trycatch"
      ],
      "settings": {
        "foreground": "#6a8cc5"
      }
    },
    {
      "name": "module,class",
      "scope": [
        "keyword.control.class",
        "keyword.control.module",
        "meta.module",
        "meta.class"
      ],
      "settings": {
        "foreground": "#6a8cc5"
      }
    },
    {
      "name": "Module / Class Name",
      "scope": [
        "entity.name.type.module",
        "entity.name.type.class",
        "support.class",
        "support.constant",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#ffb964"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#6a8cc5"
      }
    },
    {
      "name": "Interpolation",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#c6b6fe"
      }
    },
    {
      "name": "Function declaration",
      "scope": [
        "meta.method.declaration"
      ],
      "settings": {
        "foreground": "#fad07a"
      }
    },
    {
      "name": "Function brace",
      "scope": [
        "meta.brace.round"
      ],
      "settings": {
        "foreground": "#e8e8d3"
      }
    },
    {
      "name": "Function Call",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#fad07a"
      }
    },
    {
      "name": "Variable self/this",
      "scope": [
        "variable.language.self",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "Instance Variable",
      "scope": [
        "variable.other.readwrite.instance"
      ],
      "settings": {
        "foreground": "#c6b6fe"
      }
    },
    {
      "name": "Constant Symbol",
      "scope": [
        "constant.language.symbol"
      ],
      "settings": {
        "foreground": "#9c9bf7"
      }
    },
    {
      "name": "Constant Numeric",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "Constant Boolean",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#cf6a4c"
      }
    },
    {
      "name": "Regular Expression",
      "scope": [
        "punctuation.section.regexp",
        "string.regexp.interpolated"
      ],
      "settings": {
        "foreground": "#dd0093"
      }
    },
    {
      "name": "Logical Operator",
      "scope": [
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#6a8cc5"
      }
    },
    {
      "name": "Operator Rest/Spread",
      "scope": [
        "keyword.operator.rest",
        "keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#6a8cc5"
      }
    },
    {
      "name": "[JS] Block",
      "scope": [
        "punctuation.definition.block.js"
      ],
      "settings": {
        "foreground": "#e8e8d3"
      }
    },
    {
      "name": "[JS] new/var/let/const",
      "scope": [
        "meta.var.expr.js",
        "new.expr.js"
      ],
      "settings": {
        "foreground": "#fad07a"
      }
    },
    {
      "name": "[JS] Function",
      "scope": [
        "storage.type.function.js",
        "meta.function.expression.js"
      ],
      "settings": {
        "foreground": "#71b9f8"
      }
    },
    {
      "name": "[JS] Property variable",
      "scope": [
        "variable.other.property.js"
      ],
      "settings": {
        "foreground": "#c6b6fe"
      }
    },
    {
      "name": "[JS] var/let/const",
      "scope": [
        "storage.type.js"
      ],
      "settings": {
        "foreground": "#71b9f8"
      }
    },
    {
      "name": "[Ruby] other variable as method",
      "scope": [
        "variable.other.ruby"
      ],
      "settings": {
        "foreground": "#fad07a"
      }
    },
    {
      "name": "[Ruby] Start Block",
      "scope": [
        "keyword.control.start-block.ruby"
      ],
      "settings": {
        "foreground": "#6a8cc5"
      }
    },
    {
      "name": "[Ruby] Kernel Function",
      "scope": [
        "support.function.kernel.ruby"
      ],
      "settings": {
        "foreground": "#fad07a"
      }
    }
  ]
}